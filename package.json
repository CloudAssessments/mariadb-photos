{
  "name": "mariadb-photos",
  "version": "1.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CloudAssessments/mariadb-photos.git"
  },
  "description": "A demo to enable users to get and upload photos to using Node.js, MariaDB, Redis, and Docker Compose",
  "main": "app.js",
  "scripts": {
    "clean": "rm -rf node_modules; npm run db-clean",
    "db-clean": "node ./src/scripts/db-clean.js",
    "db-install": "node ./src/scripts/db-install.js",
    "dev": "nodemon ./src/bin/www",
    "lint": "eslint",
    "prod": "node ./src/scripts/db-install.js; node ./src/bin/www",
    "test": "nyc -r text -r html ava -T 300000 -c 10 'test'"
  },
  "keywords": [
    "demo",
    "photo",
    "storage",
    "filter",
    "redis",
    "mariadb",
    "docker",
    "compose"
  ],
  "author": "jonathan lee <jonl@linuxacademy.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "express": "^4.15.4",
    "jimp": "^0.2.28",
    "multer": "^1.3.0",
    "mysql2": "^1.4.1",
    "pug": "^2.0.0-rc.3",
    "redis": "^2.8.0",
    "socket.io": "^2.0.3"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "eslint": "^4.3.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-plugin-import": "^2.7.0",
    "nodemon": "^1.11.0",
    "nyc": "^11.0.3",
    "sinon": "^3.2.1"
  }
}
